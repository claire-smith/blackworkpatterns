include ../includes/mixins
include ../includes/products
doctype html
html(lang='en' class='no-js')
  head
    meta(charset="utf-8")
    meta(http-equiv='x-ua-compatible' content='ie=edge')
    <meta name="viewport" content="width=device-width, initial-scale=1">
    block vars
    title #{page_title} | Blackwork cross stitch patterns by Trunky Stitches
    meta(name="description", content=page_description)
    meta(property="og:title", content=page_title)
    meta(property="og:type", content=meta_og_type)
    meta(property="og:url" content=meta_og_url)
    meta(property="og:image" content=meta_og_image)

    link(rel='stylesheet' href='/assets/css/main.css?v=3.3')
    link(rel='stylesheet' href='/assets/bwp/css/bwp.css?v=1')

    script(type='text/javascript').
        let url = window.location.hostname
        if(url != '127.0.0.1'){
            var script = document.createElement('script');
            script.setAttribute('src', 'https://www.googletagmanager.com/gtag/js?id=G-YDMMTX9SJX');
            script.setAttribute('type', 'text/javascript');
            script.setAttribute('async', true);
            document.getElementsByTagName('head')[0].appendChild(script);

            window.onload = function(){
                window.dataLayer = window.dataLayer || [];
                function gtag(){dataLayer.push(arguments);}
                gtag('js', new Date());gtag('config', 'G-YDMMTX9SJX');
            }

        }
    <link rel="icon" href="/favicon.ico" sizes="any">
    <link rel="shortcut icon" type="image/x-icon" href="/favicon.svg">
    <link rel="icon" href="favicon.svg" type="image/svg+xml">
    <link rel="manifest" href="/manifest.webmanifest">
    <script src="/assets/js/plugins/wow.js"></script>
  body
    <!-- Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="text-center">
                    <h5 class="mb-5">Now Loading</h5>
                    <div class="loader">
                        <div class="bar bar1"></div>
                        <div class="bar bar2"></div>
                        <div class="bar bar3"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    block nav
      - var active = "index"

    include ../includes/_header.pug

    block main

    //include ../includes/_footer.pug

    <footer class="main" id="footer">
        <section class="section-padding footer-mid">
            <div class="container pt-15 pb-20">
                <div class="row">
                    <div class="col-lg-4 col-md-6">
                        <div class="widget-about font-md mb-md-5 mb-lg-0">
                            <div class="logo logo-width-1 wow fadeIn animated">
                                h5 Blackwork patterns by Trunky Stitches
                            </div>
                            <h5 class="mb-10 mt-30 fw-600 text-grey-4 wow fadeIn animated">Follow me on Instagram and Pinterest</h5>
                            <div class="mobile-social-icon wow fadeIn animated mb-sm-5 mb-md-0">
                                <a href="https://instagram.com/trunkystitches" target="_blank"><img src="/assets/imgs/theme/icons/icon-instagram.svg" alt="Instagram"></a>
                                <a href="https://www.pinterest.co.uk/trunkystitches/" target="_blank"><img src="/assets/imgs/theme/icons/icon-pinterest.svg" alt="Pinterest"></a>
                            </div>
                            <p class="wow fadeIn animated">
                                a(href="https://www.etsy.com/shop/trunkystitches?ref=bwp-f", target="_blank", rel="noopener noreferrer") Visit my Etsy shop
                            </p>

                        </div>
                    </div>
                </div>
            </div>
        </section>
        <div class="container pb-20 wow fadeIn animated">
            <div class="row">
                <div class="col-12 mb-20">
                    <div class="footer-bottom"></div>
                </div>
                <div class="col-lg-6">
                    <p class="float-md-left font-sm text-muted mb-0">&copy; 2022, <strong class="text-brand">Trunky Stitches (Claire Smith)</strong></p>
                </div>
            </div>
        </div>
    </footer>

    <!-- Vendor JS-->
    <script src="/assets/js/vendor/modernizr-3.6.0.min.js"></script>
    <script src="/assets/js/vendor/jquery-3.6.0.min.js"></script>
    <script src="/assets/js/vendor/jquery-migrate-3.3.0.min.js"></script>
    <script src="/assets/js/vendor/bootstrap.bundle.min.js"></script>
    <script src="/assets/js/plugins/slick.js"></script>
    <script src="/assets/js/plugins/jquery.syotimer.min.js"></script>

    <script src="/assets/js/plugins/jquery-ui.js"></script>
    <script src="/assets/js/plugins/perfect-scrollbar.js"></script>
    <script src="/assets/js/plugins/magnific-popup.js"></script>
    <script src="/assets/js/plugins/select2.min.js"></script>
    <script src="/assets/js/plugins/waypoints.js"></script>
    <script src="/assets/js/plugins/counterup.js"></script>
    <script src="/assets/js/plugins/jquery.countdown.min.js"></script>
    <script src="/assets/js/plugins/images-loaded.js"></script>
    <script src="/assets/js/plugins/isotope.js"></script>
    <script src="/assets/js/plugins/scrollup.js"></script>
    <script src="/assets/js/plugins/jquery.vticker-min.js"></script>
    <script src="/assets/js/plugins/jquery.theia.sticky.js"></script>
    <!-- Template  JS -->
    <script src="/assets/js/main.js?v=3.3"></script>

    block script_additional

